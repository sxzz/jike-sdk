{
  "name": "jike-sdk",
  "version": "0.27.0",
  "packageManager": "pnpm@9.15.7",
  "description": "Jike Ⓙ SDK for Node.js",
  "type": "module",
  "keywords": [
    "jike",
    "sdk"
  ],
  "license": "AGPL-3.0",
  "homepage": "https://github.com/open-jike/jike-sdk#readme",
  "bugs": {
    "url": "https://github.com/open-jike/jike-sdk/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/open-jike/jike-sdk.git"
  },
  "author": "三咲智子 <sxzz@sxzz.moe>",
  "files": [
    "*.d.ts",
    "dist"
  ],
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./*": "./*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "sideEffects": false,
  "scripts": {
    "dev": "tsx playground/main.ts",
    "build": "tsup && pnpm run build:types",
    "build:types": "tsc --emitDeclarationOnly --declaration",
    "docs:build": "typedoc",
    "docs:preview": "serve docs",
    "lint": "eslint --cache .",
    "lint:fix": "pnpm run lint --fix",
    "format": "prettier --cache --write .",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:deno": "cd tests/deno && deno run --allow-net --import-map=import_map.json --allow-env index.ts",
    "release": "bumpp && pnpm publish",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "eventemitter3": "^5.0.1",
    "ky": "^1.7.5"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.0.0",
    "@sxzz/eslint-config": "^3.17.4",
    "@sxzz/prettier-config": "^2.2.1",
    "@types/md5": "^2.3.5",
    "@types/node": "^20.17.23",
    "@vitest/ui": "^2.1.9",
    "bumpp": "^9.11.1",
    "esbuild": "^0.25.0",
    "eslint": "^9.22.0",
    "md5": "^2.3.0",
    "rollup": "^4.34.9",
    "serve": "^14.2.4",
    "sharp": "^0.33.5",
    "tsup": "^8.4.0",
    "tsx": "^4.19.3",
    "typedoc": "^0.27.9",
    "typescript": "^5.8.2",
    "vitest": "^2.1.9"
  },
  "engines": {
    "node": ">=18.12.0"
  },
  "prettier": "@sxzz/prettier-config"
}
